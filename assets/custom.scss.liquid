              transition: transform 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
              height: 2px;
              background: black;
            } */
            a:hover{
              &:after{
                transform: translateX(0); 
              }
            }
          }
        }
      }
    }
  }
  
  
  #booster-results-frx{
    .shop-booster-content{
      .shop-booster-row{
        .shop-booster-col-md-9{
          .shop-booster-row {
			margin-left: 14px;
          }
        }
      }
    }
  }
  #shopify-section-my-cart{
    .cart-header{
      .cart-header__title{
        transform: translateY(-30px); 
      }
    }
  }
}

/* Mobile version */
@media (max-width: 767px){
  .hide--mobile{display: none;}
  .full.we_loved_section {
    .container{
      .slick-prev.slick-arrow{
        position: absolute;
        left: -10px;
      }
      .slick-next.slick-arrow {
        position: absolute;
        right: -10px;
      }
    }
  }
  .head_to_cart .header_addto_cart, .head_to_cart a{
    padding: 18px 10px; 
  }
  .full.main_footer{
    margin-bottom: 30px; 
    ul{
      margin-left: 10px; 
    }
    .footer-titles{
      margin-top: 10px;
      margin-top: 10px;
      
    }
    .copy_right{
      margin-top: 0px; 
      span{
        margin-top: 10px; 
        width: 150px;
      }
      .right_pay{
        margin-top: 10px;
      }
    }
  }
  .full.fix_header{
    .full.main_header {
      .container-header{
        .main_logo{
          z-index: 999; 
        }
        .main_menu {
          z-index: 9991;
           width: unset; 
          .navbar{
            margin-left: -1px;
            margin-right: -1px;
            z-index: 1;
            .navbar-toggler{
              padding-left: 20px; 
            }
            .navbar-collapse{
              .navbar-nav{
                li{
                  &:first-child a{
                    border-top: none; 
                  }
                  a{
                      padding: 15px 20px;
                      border-top: 1px solid #b3b3b3;
                  }
                }
              }
              &:after{
                content: "";
                position: absolute;
                opacity: 0.4;
                bottom: 0;
                left: 0;
                width: 100%;
                height: 210%;
                background: black;
                transform: translateY(100%);
              }
            }
          }
        }
        .top_cart{
          width: 72px;
          z-index: 9991;
        }
      }
    }
  }
  .overlay_body{
    overflow: hidden; 
  }
  #MainContent{
    .page_margin_sect{
      margin-bottom: 50px;
      .full.shipping_info_page {
        .cont_container{
          .full.pack_order {
            padding: 0 30px;
            .shop_order{
              padding: 23px 33px;
              .box_orders_page{
                background-size: 30% !important; 
                font-size: 16px;
              }
            }
