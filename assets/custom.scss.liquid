      }
    }
    #shopify-section-home-pages{
      #ProductSection-home-pages{
        .cart_vaule{
          .par_main_page{
            .side_margggg{
              
            }
          }
        }
      }
    }
  }
  .we_loved_section{
    .container{
      .center.slider {
        .slick-list{
          .slick-track{
            .slick-slide{
              width: 265px;
              margin: 0 auto; 
/*               opacity: 0; */
            }
            .slick-current.slick-active{
/*               opacity: 1;  */
            }
          }
        }
      }
    }
  }
  #MainContent > .main__header > .vncvtm{
    margin-top: -50px;
  }
  .cart_main_page{
    margin-top: 15px;
    #shopify-section-my-cart{
