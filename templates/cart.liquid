  .sec_cart_sec .cart_bold_text {color:{{ settings.right_heading_one_two }};}
  .sec_cart_sec .thik_color {color:{{ settings.right_heading_two_price_color }}; }
  
  .trd_cart_sec  .cart_bold_text {color:{{ settings.chekout_shiping_after_color }};}
  
  .cart_main_page .cart__submit-controls .cart__submit h4 {color:{{ settings.cart_sub_heaing_color_button }};}
  .cart_main_page .cart__submit-controls .cart__submit h4:hover {color:{{ settings.cart_sub_heaing_color_button_on_hover }};}
  
  .trd_cart_sec .thik_color {color:{{ settings.chekout_shiping_after_3_color }};}
  

