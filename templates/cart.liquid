<style>
  
#vncvtw {background:{{ settings.coun_down_bg_cart }}  !important;}
 #vncvtw p {color:{{ settings.coun_down_text_color_cart }}  !important;}
 #vncvtw #vncvtd {color:{{ settings.coun_down_date_color_cart }} !important;}
  
  
  {%- unless settings.chekout_cart_after_subhaeding == nil -%}
  .cart_main_page .cart__submit-controls .cart__submit {padding: 15px 0 14px;}
  {%- endunless -%}
  
  .cart_main_page .cart__submit-controls .cart__submit {background: {{ settings.chekout_cart_color_bg }};
    color: {{ settings.chekout_cart_color_text }};}
.cart_main_page .cart__submit-controls .cart__submit:hover {color: {{ settings.chekout_cart_color_hover }};
    background: {{ settings.chekout_cart_color_bg_hover }};}
  h3.cart_f_title a {color: {{ settings.product_title_colllor }};}
