<script>
function random(min,max)
{
    return Math.floor(Math.random()*(221-1+1)+min);
}

var initial = random(1, 221);
var count = initial;

setInterval(function() {
  var variation = random(-5,5);

  count += variation
  $("#counterrr").html(count)

}, 20000)

</script>
